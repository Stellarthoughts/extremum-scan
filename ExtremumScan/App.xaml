<Application x:Class="ExtremumScan.App"
			 x:Name="ExtremumScanningMethod"
			 xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
			 xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
			 xmlns:local="clr-namespace:ExtremumScan"
			 StartupUri="MainWindow.xaml">
	<Application.Resources>
		<Style x:Key="RectangleCategory" TargetType="{x:Type Rectangle}">
            <Setter Property="RadiusX" Value="10"/>
            <Setter Property="RadiusY" Value="10"/>
            <Setter Property="StrokeThickness" Value="0"/>
            <Setter Property="Fill" Value="#FF5839AC"/>
        </Style>
        <Style x:Key="RectangleBackground" TargetType="{x:Type Rectangle}">
            <Setter Property="Fill" Value="#FFECECEC"/>
        </Style>
	</Application.Resources>
</Application>
